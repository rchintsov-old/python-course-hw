#!/usr/bin/env python3.6

import argparse

from supertool import weather

if __name__ == '__main__':

    print('')
    parser = argparse.ArgumentParser(description='Find similar files')

    parser.add_argument('city', type=str, help='target city')
    parser.add_argument('token', type=str, help='token')

    args = parser.parse_args()

    weather.get()


